{
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "plugins": ["react", "jsx-a11y"],
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "extends": [
        "airbnb",
        "airbnb-base/rules/es6",
        "airbnb-base/rules/imports",
        "prettier",
        "./eslint-base.js"
    ]
}
